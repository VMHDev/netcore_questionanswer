<log4net debug="false">
	<appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
		<!--Change folder storage log file-->
		<file type="log4net.Util.PatternString" value="logs\\" />
		<!-- ############################ -->
		<encoding value="utf-8" />
		<param name="AppendToFile" value="true" />
		<param name="RollingStyle" value="Date" />
		<param name="StaticLogFileName" value="false" />
		<param name="DatePattern" value="yyyyMMdd'.log'" />
		<layout type="log4net.Layout.PatternLayout">
			<header value="[Start Log]&#xD;&#xA;" />
			<footer value="[End Log]&#xD;&#xA;" />
			<param name="ConversionPattern" value="%d{MM/dd/yyyy HH:mm:ss ms} [%t] %-5p %c{1} %L %M- %m%n" />
		</layout>
	</appender>
	<appender name="classLogData" type="log4net.Appender.RollingFileAppender">
		<file type="log4net.Util.PatternString" value="%property{LogFileName}" />
		<encoding value="utf-8" />
		<param name="AppendToFile" value="true" />
		<param name="MaxSizeRollBackups" value="1000" />
		<param name="MaximumFileSize" value="10MB" />
		<param name="RollingStyle" value="Size" />
		<param name="StaticLogFileName" value="true" />
		<layout type="log4net.Layout.PatternLayout">
			<header value="[Start Log]&#xD;&#xA;" />
			<footer value="[End Log]&#xD;&#xA;" />
			<param name="ConversionPattern" value="%d{MM/dd/yyyy HH:mm:ss ms} [%t] %-5p %c{1} %L %M- %m%n" />
		</layout>
	</appender>
	<logger name="JOORLogsData">
		<level value="INFO" />
		<appender-ref ref="classLogData" />
	</logger>
	<root>
		<level value="ALL" />
		<appender-ref ref="RollingLogFileAppender" />
	</root>
</log4net>